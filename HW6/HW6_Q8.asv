fu_1 = @(t,y) y(2);
fu_2 = @(t,y) -y(2) + y(3) - 2*exp(-t);
fu_3 = @(t,y) y(1) + 2*y(2) + exp(-t);

f = @(t,y) [fu_1(t,y), fu_2(t,y), fu_3(t,y)];
[t,w] = rk4(f,0,1,[3,-1,1],10);
u_1_exact = @(t) -3*exp(-t) - 3*sin(t) + 6*cos(t);
u_1_approx = w(:, 1);
tt = 0:0.01:1;
plot(tt, u_1_exact(tt), t, u_1_approx, "wo")
legend("exact u1", "rk4 u1")

